{
  "headers": [
    {
        "source": "/(.*)",
        "headers" : [
            {
                "key" : "Cache-Control",
                "value" : "public, max-age=600, immutable"
            }
        ]
    }
  ],

  "functions": {
    "pages/api/resize.js": {
      "memory": 320,
      "maxDuration": 10
    },

    "pages/api/newsletter/subscribe.js": {
      "memory": 128,
      "maxDuration": 60
    }
  }
}
